#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use App\Command\CreateGitignoreCommand;
use App\Command\GetIgnorePatternsCommand;
use Symfony\Component\Console\Application;

$application = new Application();

$application->add(new GetIgnorePatternsCommand());
$application->add(new CreateGitignoreCommand());

$application->run();
